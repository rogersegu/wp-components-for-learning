{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "c4l/quote",
  "version": "1.0.0",
  "title": "Quote",
  "description": "Highlights a textual quote by some author or work external to our content.",
  "keywords": [
    "quote",
    "cite"
  ],
  "supports": {
    "html": false
  },
  "styles": [
    {
      "name": "compact-width",
      "label": "Compact width",
      "isDefault": true
    },
    {
      "name": "full-width",
      "label": "Full width"
    }
  ],
  "example": {
    "attributes": {
      "text": "Lorem ipsum dolor sit amet",
      "caption": "AUTHOR NAME, Work Title"
    }
  },
  "textdomain": "quote",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}